{
	"name": "action-logger",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"predev": "svelte-kit sync",
		"dev": "vite dev",
		"predev:host": "svelte-kit sync",
		"dev:host": "vite dev --host --port 5173",
		"prebuild": "svelte-kit sync",
		"build": "vite build",
		"prepreview": "svelte-kit sync",
		"preview": "vite preview",
		"preview:host": "vite preview --host --port 4173",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-node": "^5.4.0",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/svelte": "^5.3.0",
		"@types/better-sqlite3": "^7.6.13",
		"@types/node": "^25.0.2",
		"@vitest/coverage-v8": "^4.0.16",
		"jsdom": "^27.3.0",
		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"typescript": "^5.9.3",
		"vite": "^7.2.6",
		"vitest": "^4.0.16"
	},
	"dependencies": {
		"better-sqlite3": "^9.4.3",
		"yaml": "^2.8.2"
	}
}
